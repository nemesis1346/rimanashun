{
  "name": "rimanashun-monorepo",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "web": "npm --workspace apps/web run dev",
    "native": "npm --workspace apps/native run start",
    "backend": "npm --workspace apps/backend run start"
  },
  "devDependencies": {
    "turbo": "^2.1.3",
    "typescript": "^5.3.3"
  }
}
